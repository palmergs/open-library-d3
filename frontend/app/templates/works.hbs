<div class="container">
  <div class="row">
    <div class="twelve columns">
      <table class="table">
        <thead>
          <tr>
            <th>Title</th>
            <th>Subtitle</th>
            <th>Publishd</th>
          </tr>
        </thead>
        <tbody>
          {{#each model as |work|}}
            <tr>
              <td>{{work.title}}</td>
              <td>{{work.subtitle}}</td>
              <td>{{work.publishDate}}</td>
            </tr>
          {{/each}}
        </tbody>
      </table>
    </div>
  </div>
</div>
{{outlet}}
