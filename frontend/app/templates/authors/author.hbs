<dl>
  <dt>Name</dt>
  <dd>{{model.name}}</dd>
  <dt>Years</dt>
  <dd>{{with-unknown model.birthDate}} to {{with-unknown model.deathDate}}</dd>
  <dt>Description</dt>
  <dd>{{model.description}}</dd>
  <dt>Tags</dt>
  <dd>
    {{#each model.subjectTags as |tag|}}
      {{subject-tag model=tag}}
    {{/each}}
  </dd>
  <dt>Links</dt>
  <dd>
    {{#each model.externalLinks as |link|}}
      {{external-link model=link}}
    {{/each}}
  </dd>
  <dt>Works</dt>
  <dd>
    {{#each model.works as |work|}}
      {{link-to work.title 'works.work' work.id}}
    {{/each}}
  </dd>
</dl>
{{outlet}}
